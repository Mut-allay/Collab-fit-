{
  "version": 2,
  "buildCommand": "pnpm turbo run build:production --filter=@fitspark/web && cp -r apps/web/dist dist",
  "outputDirectory": "dist",
  "installCommand": "pnpm install --frozen-lockfile",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}